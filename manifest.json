{
    "name": "youtube thumbnail fetcher",
    "version": "1.0.0",
    "description": "fetch thumbnails from youtube and save it",
    "manifest_version": 3,
    "author": "mncc",
    "action":{
        "default_title": "fetch thumbnail of current tab"
    },
    "permissions": [
        "tabs",
        "nativeMessaging"
    ],
    "background": {
        "service_worker": "service_worker.js"
    }
}